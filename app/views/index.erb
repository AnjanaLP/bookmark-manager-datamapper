<h1>Welcome to Bookmark Manager!</h1>

<% unless current_user %>
  <form action="/users/new" method="get">
    <input type="submit" value="Signup">
  </form>

  <form action="sessions/new" method="get">
    <input type="submit" value="Sign in">
  </form>
<% end %>

<form action="/bookmarks" method="get">
  <input type="submit" value="View bookmarks">
</form>
